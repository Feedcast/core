<% content_for(:title, t(:channel, count: 2)) %>

<div class="col-xs-12 panel panel-default">
  <div class="panel-body">
      <h4 class="title-gallery"><%= t(:channel, count: @channels.count) %></h4>
      <div class="row">
        <% @channels.each do |channel| %>
          <div class="channel col-xs-6 col-sm-3 col-md-2">
            <a href="<%= channel_path(channel) %>">
              <div class="channel-img img-rounded"
                style="background-image: url(<%= image_url_for(channel) %>)"></div>
              <h3><%= channel.title %></h3>
            </a>
          </div>
        <% end %>
      </div>
  </div>
</div>
