<% content_for(:title, t(:channel, count: 2)) %>

<div class="col-xs-12 panel panel-default">
  <div class="panel-body">
    <h4 class="title-gallery"><%= t(:channel, count: @channels.count) %></h4>
    <div class="row">
      <% @channels.each do |channel| %>
        <%= render partial: "channels/item/list_item", locals: { channel: channel } %>
      <% end %>
    </div>
    <div "row pagination">
      <%= paginate @channels %>
    </div>
  </div>
</div>
