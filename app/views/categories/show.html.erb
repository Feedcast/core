<% content_for(:title, @category.title) %>

<div class="col-xs-12">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3> <%= @category.icon_html %> <%= @category.title %> </h3>
    </div>
    <div class="panel-body">
      <div class="row">
        <% @channels.each do |channel| %>
          <%= render partial: "channels/item/list_item", locals: { channel: channel } %>
        <% end %>
      </div>
    </div>
  </div>
</div>

